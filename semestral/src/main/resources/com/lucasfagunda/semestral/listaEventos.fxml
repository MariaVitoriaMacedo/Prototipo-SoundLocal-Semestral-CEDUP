<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lucasfagunda.semestral.ListaEventosController">
   <children>
      <TableView fx:id="eventosTableView" prefHeight="394.0" prefWidth="513.0">
        <columns>
          <TableColumn fx:id="principalColumn" maxWidth="2000.0" prefWidth="55.0" text="Ativo" />
          <TableColumn fx:id="nomeColumn" prefWidth="201.0" text="Nome" />
            <TableColumn fx:id="dataColumn" maxWidth="2500.0" prefWidth="75.0" text="Data" />
            <TableColumn fx:id="descricaoColumn" maxWidth="10000.0" prefWidth="75.0" text="Descrição" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="526.0" layoutY="37.0" mnemonicParsing="false" onAction="#adicionar" text="Adicionar" />
      <Button layoutX="534.0" layoutY="73.0" mnemonicParsing="false" onAction="#editar" text="Editar" />
      <Button layoutX="532.0" layoutY="108.0" mnemonicParsing="false" onAction="#excluir" text="Excluir" />
      <Button layoutX="524.0" layoutY="149.0" mnemonicParsing="false" onAction="#defPrincipal" prefHeight="48.0" prefWidth="67.0" text="Definir principal" wrapText="true" />
      <Button layoutX="531.0" layoutY="227.0" mnemonicParsing="false" onAction="#voltar" text="Voltar" />
   </children>
</AnchorPane>
